{
  "manifest_version": 2,
  "name": "Shock Liker",
  "description": "Auto push facebook like for Shock Paper",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self' https://connect.facebook.net; object-src 'self'"
}